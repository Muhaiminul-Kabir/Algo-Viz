            <JFXButton fx:id="todoButton" onAction="#loadTasks" prefHeight="107.0" prefWidth="89.0" text="TO DO" />
            <JFXButton fx:id="notesButton" onAction="#loadNotes" prefHeight="107.0" prefWidth="89.0" text="NOTES" />
            <JFXButton fx:id="settingsButton" onAction="#loadSettings" prefHeight="107.0" prefWidth="89.0" text="SETTINGS" />
            <JFXButton fx:id="progressButton" onAction="#loadProgress" prefHeight="107.0" prefWidth="89.0" text="INSIGHTS" />
         </children>
      </VBox>
   </children>
</AnchorPane>
