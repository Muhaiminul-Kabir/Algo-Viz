   <children>
      <VBox layoutY="-2.0" prefHeight="600.0" prefWidth="89.0">
         <children>
            <JFXButton fx:id="homeButton" onAction="#loadHome" prefHeight="107.0" prefWidth="89.0" text="HOME" />
